{
  "logger": {
    "path": "/var/log/dino/dinoms.log",
    "filename": "dinoms.log",
    "level": "debug",
    "rotation": "7 days",
    "retention": "1 months",
    "format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> - <level>{level: <8}</level> - <cyan>{name}:{function}:{line: <45}</cyan> - <level>{message}</level>"
  }
}