{
  "logger": {
    "path": "/var/log/dino/dinoms.log",
    "filename": "dinoms.log",
    "level": "debug",
    "rotation": "14 days",
    "retention": "12 months",
    "format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> - <level>{level: <8}</level> - <cyan>{function: <30}</cyan> - <level>{message}</level>"
  }
}